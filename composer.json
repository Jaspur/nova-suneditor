{
    "name": "webard/nova-suneditor",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Webard\\NovaSunEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webard\\NovaSunEditor\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.16"
    }
}
